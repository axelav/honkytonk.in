{
  "name": "honkytonk.in",
  "description": "https://honkytonk.in",
  "author": "Axel Anderson <axel@ftml.net>",
  "private": true,
  "scripts": {
    "build": "bankai build index.js dist && npm run extract-data && npm run render",
    "extract-data": "node ./scripts/extract-data.js",
    "inspect": "bankai inspect index.js",
    "render": "node ./scripts/render.js",
    "start": "npm run extract-data && bankai index.js -d",
    "test": "standard && test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "dependencies": {
    "choo": "^6.0.0-4",
    "choo-devtools": "^1.3.1",
    "choo-log": "^7.1.0",
    "choo-service-worker": "^2.1.6",
    "classnames": "^2.2.5",
    "tachyons": "^4.7.4",
    "youtube-component": "^1.0.1"
  },
  "devDependencies": {
    "bankai": "^9.0.0-1",
    "create-html": "^3.1.0",
    "fs-extra": "^4.0.0",
    "meta-marked": "^0.4.2",
    "minimize": "^2.1.0",
    "mkdirp": "^0.5.1",
    "recursive-readdir": "^2.2.1",
    "slug": "^0.9.1",
    "standard": "^10.0.2"
  }
}
