{
  "name": "honkytonk.in",
  "description": "https://honkytonk.in",
  "version": "1.0.0",
  "dependencies": {
    "@mdx-js/loader": "^0.15.7",
    "@mdx-js/mdx": "^0.15.7",
    "@mdx-js/tag": "^0.15.6",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "journalize": "^2.1.0",
    "lint-staged": "^8.1.0",
    "luxon": "^1.5.0",
    "mdx-provider-components": "^2.0.0",
    "next": "^7.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "remark-emoji": "^2.0.2",
    "remark-images": "^0.15.6",
    "tachyons": "^4.11.1"
  },
  "scripts": {
    "dev": "next -p 4200",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "now-build": "npm run build && npm run export -- -o dist"
  },
  "author": "Axel Anderson <axel@ftml.net>",
  "private": true,
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "default-case": "off",
      "react/prop-types": 1
    }
  },
  "lint-staged": {
    "components/**/*.{js}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ],
    "pages/**/*.{js,md}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ],
    "style.css": ["prettier --write", "git add"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
