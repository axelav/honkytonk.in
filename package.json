{
  "name": "honkytonk.in",
  "description": "https://honkytonk.in",
  "version": "1.0.0",
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.0.6",
    "classnames": "^2.2.6",
    "d3": "^6.5.0",
    "journalize": "^2.5.0",
    "luxon": "^1.25.0",
    "next": "^10.0.6",
    "pg": "^8.7.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-lazyload": "^3.2.0",
    "remark-emoji": "^2.1.0",
    "remark-images": "^2.0.0",
    "tachyons": "^4.12.0",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "dev": "next dev -p 4200",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy:dev": "now",
    "deploy:staging": "now --prod",
    "deploy:prod": "now --prod && now alias set https://honkytonkin.axelav.vercel.app honkytonk.in"
  },
  "engines": {
    "node": ">10.15.0"
  },
  "author": "Axel Anderson <axel@ftml.net>",
  "private": true,
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "default-case": "off",
      "react/prop-types": 1,
      "jsx-a11y/anchor-is-valid": 0,
      "react-hooks/rules-of-hooks": "error"
    }
  },
  "lint-staged": {
    "components/**/*.{js}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ],
    "pages/**/*.{js,md}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ],
    "style.css": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
