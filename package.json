{
  "name": "honkytonk.in",
  "scripts": {
    "build": "npm run extract-data && npm run generate-static-html",
    "extract-data": "node ./scripts/extract-data.js",
    "generate-static-html": "node ./scripts/generate-static-html.js",
    "start": "npm run extract-data && bankai index.js -d",
    "inspect": "bankai inspect index.js",
    "test": "standard && test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "dependencies": {
    "choo": "^6.0.0-4",
    "choo-devtools": "^1.3.1",
    "choo-log": "^7.1.0",
    "choo-service-worker": "^2.1.6",
    "classnames": "^2.2.5",
    "tachyons": "^4.7.4",
    "youtube-player": "^5.4.0"
  },
  "devDependencies": {
    "async": "^2.5.0",
    "bankai": "^8.1.1",
    "fs-extra": "^4.0.0",
    "meta-marked": "^0.4.2",
    "minimize": "^2.1.0",
    "recursive-readdir": "^2.2.1",
    "slugify": "^1.1.0",
    "standard": "^10.0.2"
  }
}
